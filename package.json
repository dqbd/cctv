{
  "name": "cctv",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./.next/server/start.js",
    "dev": "next dev",
    "dev:process": "node ./.next/server/start.js --dev",
    "start:next": "next start",
    "process:sync": "node ./.next/server/sync.js",
    "process:workers": "node ./.next/server/workers.js",
    "build": "next build",
    "fix": "run-p -lsc fix:*",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write \"src/**/*.ts*\"",
    "lint": "run-p -lsc lint:*",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -l \"src/**/*.ts*\""
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.21",
    "@material-ui/lab": "^5.0.0-alpha.21",
    "@use-gesture/react": "^10.2.4",
    "chokidar": "^3.5.1",
    "date-fns": "^2.27.0",
    "fluent-ffmpeg": "^2.1.2",
    "hls.js": "1.1.2",
    "knex": "^0.95.5",
    "mkdirp": "^1.0.4",
    "moment": "^2.24.0",
    "mysql2": "^2.3.3",
    "next": "^12.0.9",
    "next-seo": "^4.24.0",
    "node-fetch": "^3.1.0",
    "node-onvif": "^0.1.7",
    "nosleep.js": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "p-queue": "7.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-use-gesture": "9.1.3",
    "rimraf": "^3.0.2",
    "send": "^0.17.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/hls.js": "^1.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/mkdirp": "^1.0.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.2",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/rimraf": "^3.0.0",
    "@types/send": "^0.17.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "confusing-browser-globals": "^1.0.10",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "prettier": {
    "semi": false
  }
